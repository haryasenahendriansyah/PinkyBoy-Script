local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/SiriusSoftwareLtd/Rayfield/main/source.lua"))()

-- Notifikasi
local function Notify(title, message, image)
	Rayfield:Notify({
		Title = title,
		Content = "â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\n" .. message .. "\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”",
		Duration = 3,
		Image = image
	})
end

local function NotifySuccess(t, m) Notify(t, m, "circle-check") end
local function NotifyError(t, m) Notify(t, m, "ban") end
local function NotifyInfo(t, m) Notify(t, m, "info") end
local function NotifyWarning(t, m) Notify(t, m, "triangle-alert") end

-- Services
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local VirtualUser = game:GetService("VirtualUser")

-- Anti-AFK
LocalPlayer.Idled:Connect(function()
	VirtualUser:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
	task.wait(1)
	VirtualUser:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
end)

-- Remote setup
local net = ReplicatedStorage:WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0"):WaitForChild("net")
local rodRemote = net:WaitForChild("RF/ChargeFishingRod")
local miniGameRemote = net:WaitForChild("RF/RequestFishingMinigameStarted")
local finishRemote = net:WaitForChild("RE/FishingCompleted")

-- States
local autofish = false
local perfectCast = true
local ijump = false

-- GUI Window
local Window = Rayfield:CreateWindow({
	Name = "PinkyBoy | ğŸ¦ˆ Fish It Script",
	LoadingTitle = "Fish It Script Beta 1.0",
	LoadingSubtitle = "by Sena",
	Theme = "Amethyst",
	ConfigurationSaving = { Enabled = false },
	KeySystem = false
})

-- Developer Info
NotifyWarning("Warning!", "Please read the guide if you're confused using the features.")
local DevTab = Window:CreateTab("ğŸ‘¨â€ğŸ’» Developer", "airplay")

DevTab:CreateParagraph({
	Title = "ğŸ“Œ PinkyBoy Developer Info",
	Content = [[
